<Window x:Class="BSTWpf.View.ContainsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSTWpf.View"
        mc:Ignorable="d"
        Title="ContainsWindow" Height="150" Width="400">
    <StackPanel>
        <Label HorizontalAlignment="Center" VerticalAlignment="Center"  FontSize="25">Set Your value</Label>
        <TextBox Name="ContainsValueBox" FontSize="18"></TextBox>
        <Button FontSize="18" Click="Button_Click">Contains</Button>
    </StackPanel>
</Window>
