<Window x:Class="BSTWpf.View.CreateTreeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSTWpf.View"
        mc:Ignorable="d"
        Title="Your future tree" Height="150" Width="400">
    <StackPanel>
        <Label Height="70" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30">Set values</Label>
        <TextBox Name="valuesBox"></TextBox>
        <Button Width="70" Click="Accept_Click" IsDefault="True">Create</Button>
    </StackPanel>
</Window>
